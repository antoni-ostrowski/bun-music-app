{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch",
    "db:push": "bun drizzle-kit push",
    "db:studio": "bun drizzle-kit studio",
    "dev": "bun run watch-routes & bun --hot src/index.tsx",
    "build": "bun run generate-routes && bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.tsx",
    "compile": "bun build --compile --minify --sourcemap --bytecode src/index.tsx --outfile myapp"
  },
  "dependencies": {
    "@libsql/client": "^0.15.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-form": "^1.19.3",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@tanstack/react-router": "^1.131.27",
    "@tanstack/react-router-devtools": "^1.131.27",
    "@tanstack/react-store": "^0.7.4",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@trpc/client": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@trpc/tanstack-react-query": "^11.4.4",
    "bun-plugin-tailwind": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dev": "^0.1.3",
    "drizzle-orm": "^0.44.4",
    "lucide-react": "^0.542.0",
    "multicast-dns": "^7.2.5",
    "music-metadata": "^11.8.3",
    "node-watch": "^0.7.4",
    "open": "^10.2.0",
    "postcss": "^8.5.6",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "trpc-bun-adapter": "^1.3.0",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tanstack/router-cli": "^1.131.27",
    "@types/bun": "^1.2.20",
    "@types/cors": "^2.8.19",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "prettier": "3.6.2",
    "typescript-eslint": "^8.39.1"
  }
}
